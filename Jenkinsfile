pipeline {
         agent any
         stages {
                 stage('Stage One') {
                 steps {
                     echo 'Hi, welcome to SCM pipeline demo from SCM'
                 }
                 }
                 stage('Stage Two') {
                 steps {
                    echo('Sample testing of Stage 2')
                 }
                 }
                 stage('Stage Three') {
                
                 steps {
                       echo 'Thanks for using Jenkins Pipeline'
                 }
                 }
              }
}
